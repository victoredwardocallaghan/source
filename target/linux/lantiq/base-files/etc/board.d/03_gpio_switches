. /lib/functions/uci-defaults.sh
. /lib/functions/system.sh
. /lib/functions/lantiq.sh

board_config_update

board=$(board_name)

case "$board" in
DM200)
	ucidef_add_gpio_switch "annexa" "Annex-A" "12"
	ucidef_add_gpio_switch "annexb" "Annex-B" "15"
	;;

*)
	;;

esac

board_config_flush

exit 0
